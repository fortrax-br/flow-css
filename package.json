{
  "name": "Flow-CSS",
  "version": "0.0.1",
  "description": "Framework para resetar e aplicar estilos, adequados no HTML padrão dos browsers, permitindo ao desenvolvedor que utiize o framework, ter uma maior facilidade na hora, de estilizar sua página.",
  "main": "index.html",
  "repository": "https://notabug.org/Altos-Codigos/flow-css.git",
  "author": "AlexDK",
  "license": "GLP v3",
  "private": true,
  "scripts": {
    "css-autocompile": "npx sass --watch src/scss/flexcss.scss:src/scss/flexcss.css",
    "css-build": "postcss --use autoprefixer cssnano -o dist/css/flex.min.css src/scss/*.css"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "cssnano": "^4.1.10",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.7",
    "postcss-cli": "^8.2.0"
  },
  "dependencies": {
    "sass": "^1.29.0"
  }
}
